{
    "cSpell.words": [
        "some_new_word",
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.fixAll.shellcheck": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.formatOnSave": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.wordWrapColumn": 100,
    "files.exclude": {
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.pytest_cache": true,
        "**/XDG_CACHE_HOME": true,
        "**/__pycache__/**": true
    },
    "python.analysis.typeCheckingMode": "basic",
    "python.defaultInterpreterPath": "~/anaconda3/envs/coco/bin/python",
    "python.envFile": "${workspaceFolder}/.env",
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": false,
    "rewrap.wrappingColumn": 100,
    "shellcheck.customArgs": [
        "--source-path=SCRIPTDIR",
        "--external-sources"
    ],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#f96156",
        "activityBar.background": "#f96156",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#09f619",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#e7e7e799",
        "editorGroup.border": "#f96156",
        "panel.border": "#f96156",
        "sash.hoverBorder": "#f96156",
        "sideBar.border": "#f96156",
        "statusBar.background": "#f73325",
        "statusBar.debuggingBackground": "#25e9f7",
        "statusBar.debuggingForeground": "#15202b",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#f96156",
        "statusBarItem.remoteBackground": "#f73325",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "tab.activeBorder": "#f96156",
        "titleBar.activeBackground": "#f73325",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#f7332599",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.remoteColor": "#f73325",
}